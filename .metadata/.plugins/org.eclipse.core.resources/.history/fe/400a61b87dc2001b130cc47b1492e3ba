package com.janbask.selenium;

public class Try_Catch {

	public static void main(String[] args) {
		          //0 1  2  3 
		int a[] = {10,20,30,40};
		
		
	
		try {
			System.out.println(a[4]);
			System.out.println(" array value found ");
			
		}
		catch(Exception e) {
			
			System.out.println(" array value is not present in the index");
			e.printStackTrace();
			
			
			// perform action on fail
			
			
		}
		
		
		
		

	}

}
